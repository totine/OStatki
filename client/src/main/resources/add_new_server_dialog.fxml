<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane maxWidth="200.0" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.AddNewServerController">
   <content>
      <VBox maxHeight="300.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="244.0" text="Host">
               <font>
                  <Font name="Courier New Bold" size="22.0" />
               </font>
            </Label>
            <TextField fx:id="hostField" />
            <Label alignment="CENTER" layoutX="20.0" layoutY="20.0" prefHeight="17.0" prefWidth="244.0" text="Port">
               <font>
                  <Font name="Courier New Bold" size="22.0" />
               </font>
            </Label>
            <TextField fx:id="portField" layoutX="20.0" layoutY="47.0" />
         </children>
      </VBox>
   </content>
</DialogPane>
